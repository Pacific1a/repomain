# Telegram Bot Token
# Получить: @BotFather -> /newbot
WITHDRAWAL_BOT_TOKEN=your_bot_token_here

# Admin Telegram IDs (через запятую)
# Получить свой ID: @userinfobot
ADMIN_IDS=123456789,987654321

# Секретный ключ для связи с сервером
# Сгенерируйте случайную строку
BOT_SECRET=your-secret-key-here-change-me

# URL партнёрского сервера
SERVER_API_URL=http://localhost:3001

# Порт для API
PORT=3002
